ATTR{idProduct}=="2c11", GOTO="adb"
# Hisense U8G Smart TV (MTK MT5895)
ATTR{idProduct}=="0d02", GOTO="adbfast"
GOTO="android_usb_rules_end"
LABEL="not_Google"

